{
  "name": "sealights-cypress-plugin",
  "version": "2.0.88",
  "description": "Support and config files for Cypress to support Sealights integration.",
  "scripts": {
    "prebuild": "node -p \"'//eslint-disable-next-line quotes\\nexport const version = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "keywords": [],
  "author": "Kristijan Stefanoski<kristijan.stefanoski@sealights.io>",
  "license": "ISC",
  "devDependencies": {
    "@types/istanbul-lib-coverage": "^2.0.6",
    "cypress": "^12.17.4",
    "husky": "4.3.8",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && prettier --write . && git add -A ."
    }
  },
  "dependencies": {
    "jwt-decode": "^3.1.2",
    "sl-request": "^1.0.4",
    "slnodejs": "6.1.754"
  },
  "bugs": {
    "url": "https://github.com/Sealights/SL.OnPremis.Plugins.JavaScript/issues"
  },
  "files": [
    "tsOutputs/*",
    "support.js"
  ],
  "main": "tsOutputs/index.js",
  "gitHead": "f87848c8a5e2bcdcefd78e9d016df16c380b9c4f"
}
